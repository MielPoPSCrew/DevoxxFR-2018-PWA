{
  "index": "/index.html",
  "appData": {
    "version": "1.0.0"
  },
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html"
      ],
      "versionedFiles": [
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "prefetch",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**",
        "/homescreen.png"
      ],
      "urls": [
        "https://fonts.googleapis.com/**"
      ]
    }
  }],
  "dataGroups": [{
    "name": "devoxx-api",
    "urls":[
      "http://cfp.devoxx.fr/api/conferences/DevoxxFR2018/**",
      "https://cfp.devoxx.fr/api/conferences/DevoxxFR2018/**"
    ],
    "version": 2018,
    "cacheConfig": {
      "strategy":"performance",
      "maxSize":100000,
      "maxAge":"1d"
    }
  }]
}